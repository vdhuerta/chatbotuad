{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": ".netlify/functions-built",
    "rootDir": "./netlify/functions",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "include": [
      "netlify/functions/**/*.ts"
    ]
  }
}
